{
  "name": "@gcanossa/gas-http-api",
  "version": "1.0.2",
  "description": "Google Apps Script http api microframework",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "build": "vite build -c vite.config.ts",
    "build-e2e": "vite build -c vite.config.e2e.ts",
    "test": "vitest run",
    "pull": "clasp pull",
    "deploy": "clasp push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gcanossa/gas-http-api.git"
  },
  "keywords": [
    "gas",
    "google",
    "apps",
    "script",
    "google apps script",
    "api",
    "rest",
    "http"
  ],
  "author": "Gianmaria Canossa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanossa/gas-http-api/issues"
  },
  "homepage": "https://github.com/gcanossa/gas-http-api#readme",
  "devDependencies": {
    "@types/google-apps-script": "^1.0.78",
    "typescript": "^5.3.2",
    "vite": "^5.0.6",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.1"
  }
}
